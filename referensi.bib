isbn = {978-0-08-101975-7},
keywords = {CdTe,Colloidal synthesis,Excitons,Quantum confinement,Quantum dot,Quantum dot applications,Quantum dot solar cell,Quantum dot synthesis,Quantum nanostructure,Tunable bandgap},
pages = {59--88},
