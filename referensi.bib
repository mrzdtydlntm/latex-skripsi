file = {:home/azrimadit/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Vennila, Rajiv Kannan - 2016 - Symmetric Matrix-based Predictive Classifier for Big Data computation and information sharing in Cloud.pdf:pdf},
