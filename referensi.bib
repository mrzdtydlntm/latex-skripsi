file = {:home/azrimadit/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Kong, Liu, Wu - 2020 - A systematic review of big data-based urban sustainability research State-of-the-science and future directions.pdf:pdf},
issn = {09596526},
journal = {Journal of Cleaner Production},
keywords = {Big data,Smart city,Social media data,Urban landscape sustainability,Urban planning},
pages = {123142},
